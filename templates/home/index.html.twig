{% extends 'base.html.twig' %}

{% block title %}HomeController!{% endblock %}

{% block body %}
 

{% for article in articles %}

<div>
  <h3>{{article.title}}</h3>
  <p>
   
       <img src="{{ asset('assets/images/' ~ article.image) }}" style="width:150px;height:200px;"/>
 
  
  
  {{article.content}}</p>
</div>
{% endfor %}
{% endblock %}
